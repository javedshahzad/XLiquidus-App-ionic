{
	"appId": "com.usscyber.xl",
	"appName": "XLiquidus",
	"webDir": "www",
	"bundledWebRuntime": false,
	"version": "1.0.0",
	"server": {
		"hostname":"localhost:8100",
		"androidScheme": "https",
		"iosScheme":"https",
		"cleartext":true
	  },
	"plugins": {
		"SplashScreen": {
		  "showSpinner": true,
		  "launchShowDuration": 8000,
		  "launchAutoHide": true,
		  "spinnerColor": "#001b47",
		  "layoutName": "launch_screen",
		  "androidScaleType": "CENTER_CROP",
		  "splashFullScreen": true
		},
		"CapacitorBrowser":{
			"enable":true
		}
	  },
	  "cordova": {
		"preferences": {
		  "ScrollEnabled": "false",
		  "BackupWebStorage": "none",
		  "loadUrlTimeoutValue":"8000",
		  "StatusBarStyle":"lightcontent",
		  "OverrideUserAgent":"Mozilla/5.0 Google",
		  "StatusBarBackgroundColor":"#3A9BD0",
		  "StatusBarOverlaysWebView":"false"
		}
	  }
}
